# TeamAI Voice Platform Configuration
# Copy to .env.voice and fill in your values

# LiveKit Configuration
LIVEKIT_API_KEY=APIxxxxxxxxxxxxx
LIVEKIT_API_SECRET=secretxxxxxxxxxxxxx
LIVEKIT_WEBHOOK_URLS=

# MCP Server (uses existing context-store)
MCP_SERVER_URL=http://context-store:8000
MCP_API_KEY=

# Speech-to-Text Provider (deepgram, whisper, google)
STT_PROVIDER=deepgram
STT_API_KEY=your_stt_api_key_here

# Text-to-Speech Provider (elevenlabs, google, azure)
TTS_PROVIDER=elevenlabs
TTS_API_KEY=your_tts_api_key_here

# Service Configuration
LOG_LEVEL=info
